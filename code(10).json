        // Standard semantic colors
        "WHITE_500": "#FFFFFF",
        "BLACK_500": "#000000",
        "RED_400": "#D83C3E",         // Danger Red (light theme)
        "YELLOW_300": "#FAA61A",      // Warning Yellow
        "GREEN_360": "#3BA55D"        // Success/Online Green
