    "semanticColors": {
        // Core Text
        "HEADER_PRIMARY": [
            "#1E2124" // Dark text for headers
        ],
        "HEADER_SECONDARY": [
            "#393D42" // Slightly lighter dark text
        ],
        "TEXT_NORMAL": [
            "#2E3338" // Standard dark text
        ],
        "TEXT_MUTED": [
            "#5c6064" // Greyed-out text (e.g., channel topics)
        ],
        "TEXT_LINK": [
            "#006AEF" // Standard link blue
        ],
        // Interactive Elements
        "INTERACTIVE_NORMAL": [
            "#4f545c" // Default state for interactive elements (buttons, etc.)
        ],
        "INTERACTIVE_ACTIVE": [
            "#2E3338" // Active/pressed state - darker
        ],
        "INTERACTIVE_MUTED": [
            "#8E9297" // Disabled/muted interactive elements
        ],
        // Backgrounds - Mapped from SkeuoCord gradients (using bottom colors generally)
        "BACKGROUND_PRIMARY": [
            "#E7E6E3" // Main chat background (from --mainbg_light_bottom)
        ],
        "BACKGROUND_SECONDARY": [
            "#DAD8D2" // Side panels, user popouts (from --listbg_light_bottom)
        ],
        "BACKGROUND_SECONDARY_ALT": [
            "#DAD8D2" // Alternative secondary background
        ],
        "BACKGROUND_TERTIARY": [
            "#C6C6C3" // Outermost background (server list, settings) (from --sidebar_light_bottom)
        ],
        "BACKGROUND_ACCENT": [
            "#7289DA" // Mention highlights, etc. (using a standard light blue/blurple)
        ],
        "BACKGROUND_FLOATING": [
            "#FFFFFF" // Modals, context menus (standard white for light theme popups)
        ],
        "BACKGROUND_MOBILE_PRIMARY": [
            "#E7E6E3" // Primary view background on mobile (Chat)
        ],
        "BACKGROUND_MOBILE_SECONDARY": [
            "#DAD8D2" // Drawer/sidebar background on mobile (Channels/DMs list)
        ],
        "BACKGROUND_NESTED_FLOATING": [
            "#FFFFFF" // Popups within popups
        ],
        // Modifiers (Hover states are less relevant on mobile but needed)
        "BACKGROUND_MESSAGE_HOVER": [
            "#E0DFDC" // Slight darken of primary bg for simulated hover
        ],
        "BACKGROUND_MODIFIER_HOVER": [
            "#D0CEC9" // Slight darken of secondary bg
        ],
        "BACKGROUND_MODIFIER_ACTIVE": [
            "#C9C5BF" // Active selection (e.g., voice channel connected) (from --cell_light_bottom)
        ],
        "BACKGROUND_MODIFIER_SELECTED": [
            "#B7B6B2" // Selected item (e.g., current text channel) (from --listbg_light_top)
        ],
        "BACKGROUND_MODIFIER_ACCENT": [
            "#CDCBC6" // Subtle accents, often borders or dividers
        ],
        // Specific UI Areas
        "CHANNELTEXTAREA_BACKGROUND": [
            "#D3D3D3" // Text input area background (using --toolbar_light_top for contrast)
        ],
        "CHANNELS_DEFAULT": [
            "#4f545c" // Default channel name color in list
        ],
        "TEXT_DANGER": [
            "#D83C3E" // Standard light theme red
        ],
        "KEYBOARD": [
            "#FFFFFF" // Keyboard background (standard light)
        ],
        "CHANNEL_ICON": [
            "#5c6064" // Icons in channel list (#, lock, speaker)
        ],
        "CHAT_BACKGROUND": [
            "#E7E6E3" // Explicitly setting chat background again
        ],
         // Status Colors
        "STATUS_GREEN_600": [
            "#3BA55D" // Online
        ],
        "STATUS_YELLOW_200": [
             "#FAA61A" // Idle
         ],
        "STATUS_RED_400": [
             "#ED4245" // DND
        ],
        "STATUS_GREY_200": [
             "#747F8D" // Offline/Invisible
        ]
    },
    "rawColors": {
        // Mapping core grays and primary colors based on the SkeuoCord palette
